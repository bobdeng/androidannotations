<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.androidannotations.roboguiceexample"
    android:versionCode="1"
    android:versionName="1.0" >

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:allowBackup="true"
        android:name=".RoboGuiceExampleApplication"
        >
        <meta-data android:name="roboguice.modules" android:value="org.androidannotations.roboguiceexample.AstroModule"/>
        <activity
            android:name=".AstroGirl_"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleActivity_" />
    </application>

</manifest>